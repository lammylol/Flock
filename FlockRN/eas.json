{
  "cli": {
    "version": ">= 14.4.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "env": {
        "GOOGLE_SERVICE_INFO": "@GOOGLE_SERVICE_INFO",
        "GOOGLE_SERVICE_INFO_PATH": "./firebase/GoogleService-Info.plist"
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
