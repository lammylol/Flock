{
  "cli": {
    "version": ">= 14.4.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "env": {
        "GOOGLE_SERVICES_PLIST": "./firebase/GoogleService-Info.plist",
        "GOOGLE_SERVICES_JSON": "./firebase/google-services.json"
      }
    },
    "preview": {
      "distribution": "internal",
      "developmentClient": true,
      "env": {
        "ENV": "preview"
      }
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
