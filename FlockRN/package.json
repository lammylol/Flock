{
  "name": "flockrn",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "dev": "expo start --go",
    "dev:android": "expo start --go --android",
    "dev:ios": "expo start --go --ios",
    "dev:web": "expo start --go --web",
    "start": "expo start --dev-client",
    "test": "jest --watchAll",
    "test:ci": "jest --ci",
    "lint": "eslint .",
    "lint:ci": "expo lint",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "generate:types": "ts-node schema/generateTypes.ts",
    "clean": "rm -rf node_modules && rm -rf ios && rm -rf android",
    "clean:ios": "rm -rf ios",
    "clean:android": "rm -rf android",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|react-native-reanimated|react-native-gesture-handler)"
    ],
    "moduleNameMapper": {
      "^firebase/(.*)$": "<rootDir>/__mocks__/firebase/$1",
      "^@/(.*)$": "<rootDir>/$1"
    },
    "testEnvironment": "node",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/android/**",
      "!**/ios/**"
    ]
  },
  "dependencies": {
    "@expo/metro-config": "~0.20.0",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-firebase/app": "21.7.1",
    "@react-native-picker/picker": "2.11.0",
    "@react-native-vector-icons/common": "^11.0.0",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "dotenv": "^16.4.7",
    "expo": "^53.0.9",
    "expo-audio": "~0.4.4",
    "expo-blur": "~14.1.4",
    "expo-constants": "~17.1.6",
    "expo-dev-client": "~5.1.8",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-linear-gradient": "~14.1.4",
    "expo-linking": "~7.1.4",
    "expo-router": "~5.0.6",
    "expo-speech-recognition": "^1.1.1",
    "expo-splash-screen": "~0.30.8",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.4",
    "expo-system-ui": "~5.0.7",
    "expo-web-browser": "~14.1.6",
    "firebase": "^11.3.1",
    "later": "^1.2.0",
    "lodash": "^4.17.21",
    "openai": "^4.85.2",
    "or": "^0.2.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.2",
    "react-native-calendars": "^1.1308.1",
    "react-native-draggable-flatlist": "^4.0.1",
    "react-native-element-dropdown": "^2.12.4",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-uuid": "^2.0.3",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.13.10",
    "@types/react": "~19.0.10",
    "@types/react-test-renderer": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "jest": "~29.7.0",
    "jest-expo": "^53.0.0",
    "prettier": "^3.4.2",
    "react-test-renderer": "18.3.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3"
  },
  "private": true
}
